{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "peerDependencies": true}],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "jsx-a11y/no-autofocus": "off"
  },
  "globals": {
    "document": true
  }
}
